{
  "manifest_version": 3,
  "name": "Extensão Bootcamp - Edimar",
  "version": "1.0.0",
  "description": "Extensão mínima (MV3) para testes com Playwright e Docker.",
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "background": {
    "service_worker": "src/background/background.js"
  },
  "permissions": ["storage", "tabs"],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
